{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "TSNode",
            "type": "node",
            "request": "launch",
            "args": [
                "${workspaceRoot}/src/main.tsx"
            ],
            "runtimeArgs": [
                "--loader",
                "tsx"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}